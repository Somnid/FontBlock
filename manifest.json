{
  "name": "Font Block",
  "version": "1.0",
  "description": "Filters font DRM requests saving you bandwidth and sites money",
  "manifest_version": 2,
  "background" : {
	"scripts": [
		"js/utilities.js",
		"js/mime.js",
		"js/file-create.js", 
		"js/background.js"
	]
  },
  "permissions": [
    "<all_urls>",
	"webRequest",
	"webRequestBlocking"
  ]
}